$name?="LPCXpresso55S16"

using sysbus
mach create $name
machine LoadPlatformDescription $ORIGIN/lpcxpresso55s16.repl

$bin?=$ORIGIN/artifacts/lpcxpresso55s16_usart_interrupt_transfer.axf

showAnalyzer sysbus.flexcomm0

macro reset
"""
    sysbus LoadELF $bin
"""
runMacro $reset
